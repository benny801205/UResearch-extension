{
    "name": "UResearch",
    "version": "1.0.2",
    "manifest_version": 2,
    "description": "Your description",
    "permissions": [
      "contextMenus",
	  "tabs",
	  "https://academic.microsoft.com/paper/*"
     ],
	 
	 "content_scripts":[
    {
	"matches":["https://academic.microsoft.com/*"],
	"js":["content.js"]
	}], 
	   "icons": {
    "16": "/images/U_logo16.png",
    "32": "/images/U_logo32.png",
    "48": "/images/U_logo48.png",
    "128": "/images/U_logo128.png"
  },
	 
    "background": { 
      "scripts": ["background.js"]

    }
}